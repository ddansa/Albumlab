<html>
<head>
    <title>Hi</title>
</head>
<body>
    <h1>@Model.Name</h1>
    <p>@Model.Description</p>
    <b>Album</b>
    <ul>
        @foreach (var album in Model.Albums)
        {
            <li>
                <a href="album/details/@album.Id">@album.Title</a> (@album.Year)
            </li>
        }
    </ul>
</body>
</html>